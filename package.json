{
  "name": "marketspace-app",
  "engines": {
    "npm": ">= 8.0.0",
    "yarn": "please-use-npm",
    "node": ">= 18.0.0"
  },
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "fix:eslint": "eslint . --max-warnings=0 --fix",
    "fix:prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": [
      "npm run fix:prettier",
      "npm run fix:eslint"
    ]
  },
  "dependencies": {
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "eslint": "^8.55.0",
    "eslint-config-universe": "^12.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
